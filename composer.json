{
    "name": "signifly/laravel-travy",
    "description": "A Modular Vue-based Administration Tool",
    "homepage": "https://github.com/signifly/laravel-travy",
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Poul Jensen",
            "email": "mpj@signifly.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {"type": "vcs", "url": "https://github.com/signifly/Sanitizer"}
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "signifly/laravel-api-responder": "^1.0",
        "signifly/laravel-builder-macros": "^1.3",
        "signifly/laravel-pagination-middleware": "^1.3",
        "spatie/laravel-activitylog": "^3.8",
        "spatie/laravel-query-builder": "^2.0",
        "waavi/sanitizer": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0|~3.8.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Signifly\\Travy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Signifly\\Travy\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Signifly\\Travy\\TravyServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
