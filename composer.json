{
    "name": "signifly/laravel-travy",
    "description": "A Modular Vue-based Administration Tool",
    "homepage": "https://github.com/signifly/laravel-travy",
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Poul Jensen",
            "email": "mpj@signifly.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/signifly/Sanitizer"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.0|^7.0|^8.0",
        "signifly/laravel-api-responder": "^1.0",
        "signifly/laravel-builder-macros": "^1.4",
        "signifly/laravel-pagination-middleware": "^1.4",
        "spatie/laravel-activitylog": "^3.16",
        "spatie/laravel-query-builder": "^2.0",
        "waavi/sanitizer": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0|^10.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Signifly\\Travy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Signifly\\Travy\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Signifly\\Travy\\TravyServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
